{
  "name": "s-mcdonald/stringfluent",
  "description": "Powerful and chainable String utility for PHP.",
  "keywords": ["String" ,"String manipulation", "stringly"],
  "homepage": "https://github.com/s-mcdonald/Stringfluent",
  "license": "MIT",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Sam McDonald",
      "email": "s.mcdonald@outlook.com.au",
      "homepage": "https://github.com/s-mcdonald"
    }
  ],
  "require": {
    "php": ">=8",
    "ext-mbstring": "*",
    "ext-ctype": "*",
    "ext-http": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "mockery/mockery": "1.5.1"
  },
  "autoload": {
    "psr-4": {
      "SamMcDonald\\Stringfluent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SamMcDonald\\Stringfluent\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
